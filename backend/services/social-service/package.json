{
  "name": "social-service",
  "version": "1.0.0",
  "description": "Social features service for KampungConnect - friends, messages, and activities",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./proto ./proto/messaging.proto"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10",
    "grpc-tools": "^1.12.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}